generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["prismaSchemaFolder"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator zod {
  provider                 = "zod-prisma-types"
  output                   = "../zod-schema"
  writeBarrelFiles         = false
  createInputTypes         = false
  addInputTypeValidation   = false
  addIncludeType           = false
  addSelectType            = false
  validateWhereUniqueInput = false
  useDefaultValidators     = false
  coerceDate               = false
  writeNullishInModelTypes = true
}

enum StudentType {
  PART_TIME
  FULL_TIME
  NONE
}

enum QAStatus {
  APPROVED
  REJECTED
  COMPLETED
  INUSE
}

enum DischargeSummaryType {
  SN
  PT
  OT
  ST
  MSW
  HHA
  OTHER
}

enum LogContext {
  PATIENT
}

enum EmploymentStatus {
  FULL_TIME
  PART_TIME
  UNEMPLOYED
  SELF_EMPLOYED
  RETIRED
  MILITARY_DUTY
  UNKNOWN
}

enum Occurence {
  DAILY
  WEEKLY
  MONTHLY
  YEARLY
}

enum VisitStatus {
  UNASSIGNED
  NOT_COMPLETED
  COMPLETED
  CANCELLED
  HOSPITALIZED
  ON_HOLD
  MISSED
}

enum AdmissionPriority {
  ELECTIVE
  NEWBORN
  TRAUMA
  URGENT
  EMERGENCY
  INFORMATION_UNAVAILABLE
}

enum ProviderType {
  HOMEHEALTH
  HOSPICE
  BOTH
}

enum InsuranceSectionType {
  HOSPICE
  MEDICARE
  NON_MEDICARE
  MANAGED_CARE
  CMS
}

enum PatientStatus {
  REFERRED
  ACTIVE
  DISCHARGED
  ARCHIVED
}

enum PayerType {
  MEDICARE_PATIENT
  MEDICARE_ADV
  MANAGED_CARE
  PROF
  HOSPICE
}

enum DayType {
  SUNDAY
  MONDAY
  TUESDAY
  WEDNESDAY
  THURSDAY
  FRIDAY
  SATURDAY
}

enum VendorType {
  TWILIO
}

enum GenderType {
  MALE
  FEMALE
}

enum MaritalStatus {
  MARRIED
  SINGLE
  WIDOW
  DIVORCED
  SEPARATED
  LIFE_PARTNER
  UNKNOWN
}

enum EthnicityType {
  NATIVEAMERICAN
  ASIAN
  AFRICANAMERICAN
  HISPANIC
  NATIVEHAWAIIAN
  CAUCASIAN
  OTHERS
}

enum DmeSuppliesType {
  ORDERED
  NOTNEEDED
}

enum ServiceType {
  G0151
  G0152
  G0153
  G0157
  G0158
  G0159
  G0160
  G0161
}

enum PatientScheduleStatus {
  SKIPPED
  ARCHIVED
  ACTIVE
}

model CasbinRule {
  id        String   @id @default(uuid())
  ptype     String   @db.VarChar(100)
  v0        String?  @db.VarChar(100)
  v1        String?  @db.VarChar(100)
  v2        String?  @db.VarChar(100)
  v3        String?  @db.VarChar(100)
  v4        String?  @db.VarChar(100)
  v5        String?  @db.VarChar(100)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("casbin_rule")
}

model Provider {
  id                          String                        @id @default(uuid())
  providerName                String?                       @db.VarChar(100)
  billingName                 String?                       @db.VarChar(100)
  providerNumber              String?                       @db.VarChar(20)
  contact1                    String?                       @db.VarChar(20)
  contact2                    String?                       @db.VarChar(20)
  address1                    String?                       @db.VarChar(255)
  address2                    String?                       @db.VarChar(255)
  state                       String?                       @db.VarChar(10)
  city                        String?                       @db.VarChar(100)
  zipCode                     String?                       @db.VarChar(10)
  tpi                         String?                       @db.VarChar(100)
  clientId                    String?                       @db.VarChar(100)
  dmetpi                      String?                       @db.VarChar(100)
  identifierName              String?                       @db.VarChar(100)
  benefitCode                 String?                       @db.VarChar(100)
  npi                         String?                       @db.VarChar(100)
  taxId                       String?                       @db.VarChar(100)
  stateId                     String?                       @db.VarChar(100)
  branchId                    String?                       @db.VarChar(100)
  phone                       String?                       @db.VarChar(20)
  cellPhone                   String?                       @db.VarChar(20)
  fax                         String?                       @db.VarChar(100)
  email                       String?                       @db.VarChar(100)
  startDay                    DayType?
  taxonomy                    String?                       @db.VarChar(100)
  licenseNumber               String?                       @db.VarChar(100)
  taxonomyCode                String?                       @db.VarChar(100)
  novaetusId                  String?                       @db.VarChar(100)
  providerType                ProviderType?
  qioAddress1                 String?                       @db.VarChar(255)
  qioAddress2                 String?                       @db.VarChar(255)
  qioCity                     String?                       @db.VarChar(100)
  qioState                    String?                       @db.VarChar(100)
  qioZipCode                  String?                       @db.VarChar(10)
  qioPhone                    String?                       @db.VarChar(100)
  qioFax                      String?                       @db.VarChar(100)
  qioLocalPhone               String?                       @db.VarChar(20)
  ppiName                     String?                       @db.VarChar(100)
  ppiNpi                      String?                       @db.VarChar(100)
  ppiAddress1                 String?                       @db.VarChar(255)
  ppiAddress2                 String?                       @db.VarChar(255)
  ppiCity                     String?                       @db.VarChar(100)
  ppiState                    String?                       @db.VarChar(100)
  ppiZipCode                  String?                       @db.VarChar(100)
  ppiTaxId                    String?                       @db.VarChar(100)
  ppiTaxType                  String?                       @db.VarChar(100)
  ppiProviderId               String?                       @db.VarChar(100)
  ppiProviderNumber           String?                       @db.VarChar(20)
  ppiSecId1                   String?                       @db.VarChar(100)
  ppiSecId2                   String?                       @db.VarChar(100)
  ppiSecType1                 String?                       @db.VarChar(100)
  stateAssignedId             String?                       @db.VarChar(100)
  pressGaneyClientId          String?                       @db.VarChar(100)
  ppiSecType2                 String?                       @db.VarChar(100)
  active                      Boolean                       @default(true)
  archivedOn                  DateTime?
  createdAt                   DateTime                      @default(now())
  updatedAt                   DateTime                      @updatedAt
  UserProvider                UserProvider[]
  Vendor                      Vendor[]
  Patient                     Patient[]
  Payer                       Payer[]
  SkilledNursingNote          SkilledNursingNote[]
  Physician                   Physician[]
  Log                         Log[]
  Discipline                  Discipline[]
  PatientSchedule             PatientSchedule[]
  ScheduleVisitVerification   ScheduleVisitVerification[]
  InsurancePriorAuthorization InsurancePriorAuthorization[]
  UnscheduledVisit            UnscheduledVisit[]
  ScheduleRecurrence          ScheduleRecurrence[]
  PlanOfCare                  PlanOfCare[]
  Assessment                  Assessment[]
}

model User {
  id                    String               @id @default(uuid())
  email                 String               @unique @db.VarChar(100)
  password              String               @db.VarChar(100)
  firstName             String?              @db.VarChar(100)
  lastName              String?              @db.VarChar(100)
  middleName            String?
  licenseNo             String?              @db.VarChar(100)
  service               ServiceType?
  jobTitle              String?              @db.VarChar(100)
  taxonomy              String?              @db.VarChar(100)
  taxonomyCode          String?              @db.VarChar(100)
  notes                 String?              @db.VarChar(100)
  addressLine1          String?              @db.VarChar(100)
  addressLine2          String?              @db.VarChar(100)
  country               String?              @db.VarChar(100)
  state                 String?              @db.VarChar(100)
  city                  String?              @db.VarChar(100)
  postalCode            String?              @db.VarChar(100)
  homePhone             String?              @db.VarChar(100)
  cellPhone             String?              @db.VarChar(100)
  fax                   String?              @db.VarChar(100)
  memo                  String?              @db.VarChar(255)
  image                 Media?               @relation(fields: [mediaId], references: [id])
  active                Boolean              @default(true)
  archivedOn            DateTime?
  createdAt             DateTime             @default(now())
  updatedAt             DateTime             @updatedAt
  userHistory           UserHistory?         @relation("user-history")
  UserProvider          UserProvider[]
  mediaId               String?
  Patient               Patient[]
  PatientAdmission      PatientAdmission[]
  PatientSchedule       PatientSchedule[]
  PatientInsurance      PatientInsurance[]
  RelatedCaregiver      RelatedCaregiver[]
  UnscheduledVisit      UnscheduledVisit[]
  SkilledNursingNote    SkilledNursingNote[]
  PlanOfCareCaseManager PlanOfCare[]         @relation("poc-case-manager")
  PlanOfCareCaregiver   PlanOfCare[]         @relation("poc-caregiver")
  Assessment            Assessment[]
  PatientScheduler      PatientSchedule[]    @relation("scheduler")
}

model UserHistory {
  id                      String                   @id @default(uuid())
  userId                  String                   @unique
  user                    User                     @relation("user-history", fields: [userId], references: [id])
  socialSecurity          String?
  dob                     DateTime?
  driversLicense          License?                 @relation("drivers", fields: [driversLicenseId], references: [id])
  professionalLicense     License?                 @relation("professional", fields: [professionalLicenseId], references: [id])
  driversLicenseId        String?
  professionalLicenseId   String?
  hireDate                DateTime?
  lastDate                DateTime?
  evaluationDueDate       DateTime?
  oigMonthlyUpdate        String?
  yearlyEvaluationDueDate DateTime?
  caregiverCertifications CaregiverCertification[]
  media                   Media[]
  criminalCheckDueDate    DateTime?
  screeningDueDate        DateTime?
  lastCPRTraining         DateTime?
  CPRExpiration           DateTime?
  insuranceExpiration     DateTime?
  lastAidRegistry         DateTime?
  lastMisconductRegistry  DateTime?
  greenCardExpiration     DateTime?
  active                  Boolean                  @default(true)
  archivedOn              DateTime?
  createdAt               DateTime                 @default(now())
  updatedAt               DateTime                 @updatedAt
}

model CaregiverCertification {
  id            String       @id @default(uuid())
  certification String?      @db.VarChar(255)
  expires       DateTime?
  userHistory   UserHistory? @relation(fields: [userHistoryId], references: [id])
  userHistoryId String?
}

model License {
  id                             String        @id @default(uuid())
  name                           String?       @db.VarChar(100)
  expires                        DateTime?
  UserHistoryDriversLicense      UserHistory[] @relation("drivers")
  UserHistoryProfessionalLicense UserHistory[] @relation("professional")
}

model Media {
  id                                 String                      @id @default(uuid())
  fileType                           String?                     @db.VarChar(100)
  fileName                           String?                     @db.VarChar(100)
  mediaId                            String                      @unique
  src                                String?
  alt                                String?
  size                               Float?
  updatedAt                          DateTime                    @updatedAt
  createdAt                          DateTime                    @default(now())
  archivedOn                         DateTime?
  active                             Boolean                     @default(true)
  UserHistory                        UserHistory?                @relation(fields: [userHistoryId], references: [id])
  userHistoryId                      String?
  User                               User[]
  ScheduleVisitVerification          ScheduleVisitVerification[]
  DischargeSummary                   DischargeSummary[]
  UnscheduledVisitPatientSignature   UnscheduledVisit[]          @relation("patient-signature")
  UnscheduledVisitCaregiverSignature UnscheduledVisit[]          @relation("caregiver-signature")
  QAPatientSignature                 QASignature[]               @relation("patient-signature")
  QANurseSignature                   QASignature[]               @relation("nurse-signature")
  PlanOfCare                         PlanOfCare[]
}

model UserProvider {
  id         String   @id @default(uuid())
  user       User     @relation(fields: [userId], references: [id])
  userId     String   @db.VarChar(100)
  provider   Provider @relation(fields: [providerId], references: [id])
  providerId String   @db.VarChar(100)
  createdAt  DateTime @default(now())
  updatedAt  DateTime @default(now()) @updatedAt
}

model Vendor {
  id          String     @id @default(uuid())
  name        VendorType
  provider    Provider   @relation(fields: [providerId], references: [id])
  providerId  String     @db.VarChar(100)
  credentials Json?
  active      Boolean    @default(true)
  createdAt   DateTime   @default(now())
  updatedAt   DateTime   @updatedAt
}

model Group {
  id        String   @id @default(uuid())
  name      String   @unique @db.VarChar(100)
  active    Boolean  @default(true)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Permission {
  id        String   @id @default(uuid())
  name      String   @unique @db.VarChar(100)
  active    Boolean  @default(true)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Patient {
  id                       String                    @id @default(uuid())
  firstName                String?                   @db.VarChar(100)
  middleInitial            String?                   @db.VarChar(100)
  lastName                 String?                   @db.VarChar(100)
  patientNo                String?                   @db.VarChar(100)
  provider                 Provider?                 @relation(fields: [providerId], references: [id])
  providerId               String?
  dob                      DateTime?
  suffix                   String?                   @db.VarChar(100)
  ssn                      String?                   @db.VarChar(100)
  phone                    String?                   @db.VarChar(100)
  medicaidNumber           String?                   @db.VarChar(100)
  medicareNumber           String?                   @db.VarChar(100)
  address1                 String?                   @db.VarChar(255)
  address2                 String?                   @db.VarChar(255)
  country                  String?                   @db.VarChar(100)
  city                     String?                   @db.VarChar(100)
  state                    String?                   @db.VarChar(100)
  zip                      String?                   @db.VarChar(100)
  gender                   GenderType?
  maritalStatus            MaritalStatus?
  workersComp              String?                   @db.VarChar(100)
  race                     EthnicityType?
  referralSource           String?                   @db.VarChar(100)
  physician                Physician?                @relation(fields: [physicianId], references: [id])
  physicianId              String?
  dme                      String?                   @db.VarChar(100)
  controlNumber            String?                   @db.VarChar(100)
  admissionSOC             DateTime?
  notAPhysician            Boolean                   @default(false)
  notMedicareNumber        Boolean                   @default(false)
  CBSACode                 String?                   @db.VarChar(100)
  admissionSource          String?                   @db.VarChar(100)
  admissionPriority        AdmissionPriority?
  authorizationNumber      String?                   @db.VarChar(100)
  employmentStatus         EmploymentStatus?
  faceToFace               DateTime?
  taxonomy                 String?                   @db.VarChar(100)
  taxonomyCode             String?                   @db.VarChar(100)
  conditionRelation        String[]
  county                   String?                   @db.VarChar(100)
  autoAccidentState        String?                   @db.VarChar(100)
  sharePatient             Boolean                   @default(false)
  dmeSupplies              DmeSuppliesType?
  student                  StudentType?
  supervisingPhysician     String?                   @db.VarChar(100)
  supervisingPhysicianNpi  String?                   @db.VarChar(100)
  status                   PatientStatus?
  createdAt                DateTime                  @default(now())
  updatedAt                DateTime                  @updatedAt
  active                   Boolean                   @default(true)
  archivedOn               DateTime?
  patientAuthorization     PatientAuthorization[]
  patientInsurance         PatientInsurance[]
  patientCommercial        PatientCommercial?
  patientEmergencyContact  PatientEmergencyContact?
  patientReferralSource    PatientReferralSource?
  patientMedication        PatientMedication?
  patientAccessInformation PatientAccessInformation?
  patientAdmission         PatientAdmission[]
  caregiver                User?                     @relation(fields: [caregiverId], references: [id])
  caregiverId              String?
  PatientSchedule          PatientSchedule[]
  patientPolicyHolder      PatientPolicyHolder?
  pan                      String?                   @db.VarChar(100)
  dnr                      String?                   @db.VarChar(100)
  infectionControl         String?                   @db.VarChar(100)
  admitInfection           String?                   @db.VarChar(100)
  transferredFrom          Boolean                   @default(false)
  PatientFrequency         PatientFrequency[]
  PatientDiscipline        PatientDiscipline[]
  DischargeSummary         DischargeSummary[]
  PatientOtherInfo         PatientOtherInfo[]
  UnscheduledVisit         UnscheduledVisit[]
  SkilledNursingNote       SkilledNursingNote[]
  PlanOfCare               PlanOfCare[]
  Assessment               Assessment[]
}

model PatientAuthorization {
  id               String    @id @default(uuid())
  startDate        DateTime?
  endDate          DateTime?
  status           String?   @db.VarChar(100)
  insurance        String?   @db.VarChar(225)
  number           String?   @db.VarChar(225)
  visitsAuthorized String?   @db.VarChar(225)
  sn               String?   @db.VarChar(100)
  pt               String?   @db.VarChar(100)
  ot               String?   @db.VarChar(100)
  st               String?   @db.VarChar(100)
  msw              String?   @db.VarChar(100)
  rn               String?   @db.VarChar(100)
  lvn              String?   @db.VarChar(100)
  caregiver        String?   @db.VarChar(100)
  hha              String?   @db.VarChar(100)
  rm               String?   @db.VarChar(100)
  assLiv           String?   @db.VarChar(100)
  empAs            String?   @db.VarChar(100)
  peer             String?   @db.VarChar(100)
  counselling      String?   @db.VarChar(100)
  sud              String?   @db.VarChar(100)
  sudg             String?   @db.VarChar(100)
  nurse            String?   @db.VarChar(100)
  psychRe          String?   @db.VarChar(100)
  psychRehg        String?   @db.VarChar(100)
  transp           String?   @db.VarChar(100)
  supEmp           String?   @db.VarChar(100)
  shl              String?   @db.VarChar(100)
  hhc              String?   @db.VarChar(100)
  sls              String?   @db.VarChar(100)
  comment          String?   @db.VarChar(255)
  Patient          Patient?  @relation(fields: [patientId], references: [id])
  patientId        String?
}

model Payer {
  id               String             @id @default(uuid())
  name             String?            @db.VarChar(100)
  phone            String?            @db.VarChar(100)
  fax              String?            @db.VarChar(100)
  provider         Provider           @relation(fields: [providerId], references: [id])
  providerId       String             @db.VarChar(100)
  address          String?            @db.VarChar(100)
  city             String?            @db.VarChar(100)
  state            String?            @db.VarChar(100)
  zip              String?            @db.VarChar(100)
  npi              String?            @db.VarChar(100)
  active           Boolean            @default(true)
  archivedOn       DateTime?
  createdAt        DateTime           @default(now())
  updatedAt        DateTime           @updatedAt
  PatientInsurance PatientInsurance[]
}

model PatientInsurance {
  id                       String                @id @default(uuid())
  type                     InsuranceSectionType?
  status                   Boolean?              @default(false)
  daysPerEpisode           String?               @default("0") @db.VarChar(100)
  noOfVisitAuthorized      String?               @db.VarChar(100)
  serviceRequired          String[]
  company                  String?               @db.VarChar(100)
  payerId                  String?               @db.VarChar(100)
  insuredId                String?               @db.VarChar(100)
  clearingClaims           String?               @db.VarChar(100)
  patient                  Patient?              @relation(fields: [patientId], references: [id])
  patientId                String?
  payer                    Payer?                @relation(fields: [payerId], references: [id])
  payerResponsibility      String?               @db.VarChar(100)
  memberId                 String?               @db.VarChar(100)
  groupName                String?               @db.VarChar(100)
  groupNumber              String?               @db.VarChar(100)
  insuranceCaseManager     User?                 @relation(fields: [insuranceCaseManagerId], references: [id])
  insuranceCaseManagerId   String?
  billType                 String?               @db.VarChar(100)
  assignBenefits           String?               @db.VarChar(100)
  providerAcceptAssignment String?               @db.VarChar(100)
  effectiveFrom            DateTime?
  effectiveThrough         DateTime?
  patientRelationship      String?
  lastName                 String?               @db.VarChar(100)
  firstName                String?               @db.VarChar(100)
  middleName               String?               @db.VarChar(100)
  suffix                   String?               @db.VarChar(100)
  dob                      String?               @db.VarChar(100)
  sex                      String?               @db.VarChar(100)
  address1                 String?               @db.VarChar(100)
  address2                 String?               @db.VarChar(100)
  city                     String?               @db.VarChar(100)
  state                    String?               @db.VarChar(100)
  zip                      String?               @db.VarChar(100)
  relationshipToPatient    String?               @db.VarChar(100)
  relatedCaregiver         RelatedCaregiver[]
  copayType                String?               @db.VarChar(100)
  copayAmount              String?               @db.VarChar(100)
  comment                  String?               @db.VarChar(100)
  active                   Boolean               @default(true)
  archivedOn               DateTime?

  createdAt                   DateTime                      @default(now())
  updatedAt                   DateTime                      @updatedAt
  InsurancePriorAuthorization InsurancePriorAuthorization[]
}

model PatientFrequency {
  id               String      @id @default(uuid())
  patientId        String
  patient          Patient     @relation(fields: [patientId], references: [id])
  discipline       Discipline? @relation(fields: [disciplineId], references: [id])
  disciplineId     String?
  visit            String?     @db.VarChar(100)
  perDay           String?     @db.VarChar(100)
  effectiveFrom    DateTime?
  effectiveThrough DateTime?
  comment          String?     @db.VarChar(100)
  active           Boolean     @default(true)
  archivedOn       DateTime?
  createdAt        DateTime    @default(now())
  updatedAt        DateTime    @updatedAt
}

model RelatedCaregiver {
  id                 String            @id @default(uuid())
  caregiver          User?             @relation(fields: [caregiverId], references: [id])
  caregiverId        String?
  relationShip       String?
  PatientInsurance   PatientInsurance? @relation(fields: [patientInsuranceId], references: [id])
  patientInsuranceId String?
}

model PatientOtherInfo {
  id                         String           @id @default(uuid())
  comment                    String?          @db.VarChar(100)
  patient                    Patient          @relation(fields: [patientId], references: [id])
  patientId                  String           @unique
  noPublicity                Boolean          @default(false)
  telephony                  Boolean          @default(false)
  referralDate               DateTime?
  region                     String?          @db.VarChar(100)
  sentDate                   DateTime?
  receivedDate               DateTime?
  pharmacyName               String?          @db.VarChar(100)
  pharmacyPhone              String?          @db.VarChar(100)
  pharmacyFax                String?          @db.VarChar(100)
  excludeCareConnect         Boolean          @default(false)
  evacuationLevel            String?          @db.VarChar(100)
  physician                  Physician?       @relation(fields: [physicianId], references: [id])
  otherPhysician             OtherPhysician[]
  releaseInformation         String?          @db.VarChar(100)
  patientSignatureSourceCode String?          @db.VarChar(100)
  patientConditions          String[]
  patientConditionState      String?          @db.VarChar(100)
  patientConditionDate       DateTime?
  createdAt                  DateTime         @default(now())
  updatedAt                  DateTime         @updatedAt
  physicianId                String?
}

model OtherPhysician {
  id                 String            @id @default(uuid())
  physician          Physician?        @relation(fields: [physicianId], references: [id])
  physicianId        String?
  comment            String?           @db.VarChar(255)
  PatientOtherInfo   PatientOtherInfo? @relation(fields: [patientOtherInfoId], references: [id])
  patientOtherInfoId String?
}

model PatientCommercial {
  id                          String      @id @default(uuid())
  insuranceInformation        String[]
  payId                       String?     @db.VarChar(100)
  policyHolder                String?     @db.VarChar(100)
  insuredHolder               String?     @db.VarChar(100)
  uniqueId                    String?     @db.VarChar(100)
  gender                      GenderType?
  dob                         DateTime?
  address                     String?     @db.VarChar(255)
  state                       String?     @db.VarChar(100)
  city                        String?     @db.VarChar(100)
  country                     String?     @db.VarChar(100)
  zip                         String?     @db.VarChar(100)
  phone                       String?     @db.VarChar(100)
  employer                    String?     @db.VarChar(100)
  groupName                   String?     @db.VarChar(100)
  groupNumber                 String?     @db.VarChar(100)
  patient                     Patient?    @relation(fields: [patientId], references: [id])
  patientId                   String?     @unique
  isOtherBenefitPlan          Boolean     @default(false)
  otherInsured                String?     @db.VarChar(100)
  otherBenefitPlanEmployer    String?     @db.VarChar(100)
  otherBenefitPlanDob         DateTime?
  otherBenefitPlanGroupName   String?     @db.VarChar(100)
  otherBenefitPlanGroupNumber String?     @db.VarChar(100)
  otherBenefitPlanGender      GenderType?
}

model PatientEmergencyContact {
  id                  String   @id @default(uuid())
  nextOfKinName       String?  @db.VarChar(100)
  nextOfKinRelation   String?  @db.VarChar(100)
  nextOfKinPhone      String?  @db.VarChar(100)
  nextOfKinExt        String?  @db.VarChar(100)
  nextOfKinAddress    String?  @db.VarChar(100)
  homePet             String?  @db.VarChar(100)
  livesWith           String?  @db.VarChar(100)
  smokesInHome        String?  @db.VarChar(100)
  location            String?  @db.VarChar(100)
